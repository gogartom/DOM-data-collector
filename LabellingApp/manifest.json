{
  "name": "Hello World!",
  "description": "My first Chrome App.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js","jquery.js","content_script.js","window_script.js"]
    }
  },
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" },
  "permissions": [
    "webview",
    "http://*/",
    "https://*/",
    {"fileSystem": ["write"]}
  ]
}